<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<TargetFramework>netstandard2.0</TargetFramework>
	  <Version>1.0.1</Version>
	  <Authors>Arda Terekeci</Authors>
	  <Company></Company>
	  <Product>Resultant</Product>
	  <PackageId>Resultant</PackageId>
	  <Description>A robust and flexible C# library for implementing the Result pattern.</Description>
	  <PackageTags>C#;ResultPattern;ErrorHandling</PackageTags>
	  <RepositoryUrl>https://github.com/adomorn/Resultant</RepositoryUrl>
	  <RepositoryType>git</RepositoryType>
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
	  <PackageReadmeFile>README.md</PackageReadmeFile>
	  <Title>Resultant</Title>
  </PropertyGroup>

  <ItemGroup>
	<None Include="..\README.md">
	  <Pack>True</Pack>
	  <PackagePath>\</PackagePath>
	</None>
  </ItemGroup>

</Project>
